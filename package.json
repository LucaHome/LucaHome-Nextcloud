{
	"name": "wirelesscontrol",
	"description": "Wireless Control lets you control 433MHz sockets within Nextcloud while running on a raspberry",
	"version": "1.3.2",
	"author": {
		"name": "Jonas Schubert",
		"email": "guepardoapps@gmail.com"
	},
	"license": "MIT",
	"private": true,
	"homepage": "https://github.com/LucaHome/WirelessControl-Nextcloud",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build_vue": "vue-cli-service build",
		"dev": "webpack --config webpack.dev.js",
		"watch": "webpack --progress --watch --config webpack.dev.js",
		"build": "webpack --progress --hide-modules --config webpack.prod.js",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src",
		"stylelint:fix": "stylelint src --fix",
		"test": "mocha-webpack --webpack-config webpack.test.js --interactive false --require tests/setup.js \"tests/js/**/*.spec.js\"",
		"test:watch": "mocha-webpack -w --webpack-config webpack.test.js --interactive false --require tests/setup.js \"tests/js/**/*.spec.js\""
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:LucaHome/WirelessControl-Nextcloud.git"
	},
	"keywords": [
		"nextcloud",
		"wirelesscontrol",
		"433MHz"
	],
	"bugs": "https://github.com/LucaHome/WirelessControl-Nextcloud/issues",
	"contributors": [],
	"dependencies": {
		"@babel/polyfill": "7.4.3",
		"axios": "0.18.0",
		"debounce": "1.2.0",
		"nextcloud-server": "0.15.9",
		"nextcloud-vue": "0.9.7",
		"vue": "2.6.10",
		"vue-material": "1.0.0-beta-10.2",
		"vue-router": "3.0.4",
		"vuelidate": "0.7.4",
		"vuex": "3.1.0",
		"vuex-router-sync": "5.0.0"
	},
	"browserslist": [
		"last 2 versions",
		"ie >= 11"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.4.3",
		"@babel/plugin-proposal-object-rest-spread": "7.4.3",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/preset-env": "7.4.3",
		"@vue/cli-plugin-babel": "3.6.0",
		"@vue/cli-plugin-eslint": "3.6.0",
		"@vue/cli-service": "3.6.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-core": "6.26.3",
		"babel-eslint": "10.0.1",
		"babel-jest": "24.7.1",
		"babel-loader": "8.0.5",
		"chai": "4.2.0",
		"css-loader": "2.1.1",
		"eslint": "5.16.0",
		"eslint-config-standard": "12.0.0",
		"eslint-import-resolver-webpack": "0.11.1",
		"eslint-loader": "2.1.2",
		"eslint-plugin-import": "2.17.1",
		"eslint-plugin-node": "8.0.1",
		"eslint-plugin-promise": "4.1.1",
		"eslint-plugin-standard": "4.0.0",
		"eslint-plugin-vue": "5.2.2",
		"file-loader": "3.0.1",
		"jest": "24.7.1",
		"jest-serializer-vue": "2.0.2",
		"jsdom": "14.0.0",
		"jsdom-global": "3.0.2",
		"less": "3.9.0",
		"less-loader": "4.1.0",
		"mocha": "6.1.3",
		"mocha-webpack": "2.0.0-beta.0",
		"node-sass": "4.11.0",
		"sass-loader": "7.1.0",
		"stylelint": "10.0.0",
		"stylelint-config-recommended-scss": "3.2.0",
		"stylelint-scss": "3.5.4",
		"stylelint-webpack-plugin": "0.10.5",
		"svg-sprite": "1.5.0",
		"vue-jest": "3.0.4",
		"vue-loader": "15.7.0",
		"vue-template-compiler": "2.6.10",
		"webpack": "4.30.0",
		"webpack-cli": "3.3.0",
		"webpack-merge": "4.2.1",
		"webpack-node-externals": "1.7.2"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"collectCoverage": true,
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,vue}",
			"!**/node_modules/**"
		],
		"coverageReporters": [
			"json",
			"text",
			"html"
		]
	}
}
