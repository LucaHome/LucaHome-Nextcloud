{
  "name": "lucahome",
  "description": "Nextcloud - LucaHome",
  "version": "0.2.0",
  "author": {
    "name": "Jonas Schubert",
    "email": "guepardoapps@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/LucaHome/LucaHome-Nextcloud",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LucaHome/LucaHome-Nextcloud.git"
  },
  "bugs": "https://github.com/LucaHome/LucaHome-Nextcloud/issues",
  "contributors": [],
  "dependencies": {
    "axios": "0.18.0",
    "nextcloud-axios": "0.1.3",
    "nextcloud-vue": "0.6.1",
    "vue": "2.6.6",
    "vue-material": "1.0.0-beta-10.2",
    "vuelidate": "0.7.4",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@vue/cli-plugin-babel": "3.4.0",
    "@vue/cli-plugin-eslint": "3.4.0",
    "@vue/cli-service": "3.4.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "eslint": "5.13.0",
    "eslint-plugin-vue": "5.1.0",
    "jest": "24.1.0",
    "jest-serializer-vue": "2.0.2",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "vue-jest": "3.0.3",
    "vue-loader": "15.6.2",
    "vue-template-compiler": "2.6.6",
    "webpack": "4.29.4",
    "webpack-cli": "3.2.3",
    "webpack-merge": "4.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "json",
      "text",
      "html"
    ]
  }
}
